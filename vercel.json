{
  "buildCommand": "yarn install --ignore-engines && yarn build",
  "installCommand": "yarn install --ignore-engines",
  "build": {
    "env": {
      "NODE_OPTIONS": "--openssl-legacy-provider"
    }
  },
  "rewrites": [
    {
      "source": "/api/:match*",
      "destination": "https://music-api.byebug.cn/:match*"
    }
  ]
}
